{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "build_backend",
            "command": "dotnet",
            "type": "process",
            "args": [
                "build",
                "${workspaceFolder}/HostedServiceDemo/HostedServiceDemo.csproj"
            ],
            "problemMatcher": "$msCompile"
        },
        {
            "label": "build_web",
            "command": "dotnet",
            "type": "process",
            
            "args": [
                "build",
                "${workspaceFolder}/WebDemo/WebDemo.csproj"
            ],
            "problemMatcher": "$msCompile"
        },
        {
            "label": "build",
            "dependsOn": [
                "build_backend",
                "build_web"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "problemMatcher": "$msCompile"
        }
    ]
}